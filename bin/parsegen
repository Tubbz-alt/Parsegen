#! /usr/bin/env python3

import sys

from parsegen import parse, output

def main(args):
	for a in args:
		grammar = parse.parse_file(a)
		with open(a + '.out', 'w') as file:
			output.write_grammar(
				grammar.header, grammar.expansions, grammar.user_code, file)

if __name__ == '__main__':
	sys.exit(main(sys.argv[1:]))